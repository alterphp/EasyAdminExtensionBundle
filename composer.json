{
    "name": "alterphp/easyadmin-extension-bundle",
    "type": "symfony-bundle",
    "description": "This bundle provides some extensions to javiereguiluz/easyadmin-bundle",
    "keywords": ["backend", "admin", "generator"],
    "homepage": "https://github.com/alterphp/EasyAdminExtensionBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Pierre-Charles Bertineau",
            "email": "pc.bertineau@alterphp.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "doctrine/collections": "^1.5",
        "doctrine/common": "^2.8",
        "doctrine/doctrine-bundle": "^1.8|^2.0",
        "doctrine/orm": "^2.6",
        "easycorp/easyadmin-bundle": "^2.0",
        "league/uri-manipulations": "^1.3",
        "league/uri-schemes": "^1.2",
        "symfony/config": "^4.1",
        "symfony/dependency-injection": "^4.1|^5.0",
        "symfony/event-dispatcher": "^4.1|^5.0",
        "symfony/form": "^4.1|^5.0",
        "symfony/framework-bundle": "^4.1|^5.0",
        "symfony/http-foundation": "^4.1|^5.0",
        "symfony/http-kernel": "^4.1|^5.0",
        "symfony/property-access": "^4.1|^5.0",
        "symfony/validator": "^4.1|^5.0",
        "twig/twig": "^2.4"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "friendsofphp/php-cs-fixer": "^2.11",
        "php-coveralls/php-coveralls": "^2.0",
        "phpstan/phpstan": "^0.9.2",
        "phpunit/phpunit": "~5",
        "psr/log": "~1.0",
        "symfony/browser-kit": "^4.1|^5.0",
        "symfony/console": "^4.1|^5.0",
        "symfony/css-selector": "^4.1|^5.0",
        "symfony/dom-crawler": "^4.1|^5.0",
        "symfony/phpunit-bridge": "^4.1|^5.0",
        "symfony/polyfill": "^1.7",
        "symfony/security-bundle": "^4.1|^5.0",
        "symfony/templating": "^4.1|^5.0",
        "symfony/thanks": "^1.0",
        "symfony/twig-bundle": "^4.1|^5.0",
        "symfony/var-dumper": "^4.1|^5.0"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "ext-intl": "1.0"
        }
    },
    "autoload": {
        "psr-4": {
            "AlterPHP\\EasyAdminExtensionBundle\\": "src/"
        },
        "exclude-from-classmap": ["/Tests/"]
    },
    "autoload-dev": {
        "psr-4": {
            "AppTestBundle\\": "tests/Fixtures/AppTestBundle/",
            "AlterPHP\\EasyAdminExtensionBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
